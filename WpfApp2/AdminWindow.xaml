        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" x:Class="WpfApp2.AdminWindow"
        mc:Ignorable="d"
        Title="AdminWindow" Height="744" Width="852" Background="{x:Null}">
    <Border BorderThickness="1" Background="White" CornerRadius="20,20,20,20">
        <Grid Margin="-1,9,-1,-9">
            <Grid.RowDefinitions>
                <RowDefinition Height="71"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Border BorderBrush="Black" BorderThickness="0,0,0,1" RenderTransformOrigin="0.5,0.5" Margin="-14,-1,7,20">
                <Border.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="-0.225"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Border.RenderTransform>
                <Label Content="Admin Portal" Margin="75,0,684,27" Foreground="#FF00104D" FontWeight="SemiBold"/>
            </Border>
            <Label Content="Welcome Back!, Admin" HorizontalAlignment="Left" Margin="61,16,0,0" VerticalAlignment="Top" FontSize="8" Foreground="#FF797979" Height="22" Width="91"/>
            <Image HorizontalAlignment="Left" Height="44" VerticalAlignment="Top" Width="49" Source="/mcm logo.png" RenderTransformOrigin="1.025,-0.28" Margin="20,2,0,0"/>
            <Button Content="Log out" HorizontalAlignment="Left" Margin="709,10,0,0" VerticalAlignment="Top" Height="20" Width="72" Click="Button_Click">
                <Button.Background>
                    <ImageBrush/>
                </Button.Background>
            </Button>
            <Grid Grid.Row="1" Margin="0,0,0,10">
                <Border BorderBrush="Black" BorderThickness="1" Margin="102,47,102,539" CornerRadius="15,15,15,15"/>
                <TabControl Margin="40,54,40,53" BorderBrush="{x:Null}" Background="{x:Null}">
                    <TabItem Header="Overview" Background="{x:Null}" Margin="109,-7,-134,5" BorderThickness="5,0,0,0" BorderBrush="{x:Null}">
                        <Grid Height="510" Margin="0,0,-1,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="121*"/>
                                <RowDefinition Height="389*"/>
                            </Grid.RowDefinitions>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Border BorderBrush="Black" BorderThickness="1" Margin="10,34,10,33" CornerRadius="5,5,5,5"/>
                                <Label Content="Pending Appointmens" HorizontalAlignment="Left" Margin="49,35,0,0" VerticalAlignment="Top"/>
                                <Image HorizontalAlignment="Left" Height="30" Margin="15,0,0,0" VerticalAlignment="Center" Width="36" Source="/times.jpg"/>
                                <Border BorderBrush="Black" BorderThickness="1" Margin="10,35,10,33" Grid.Column="1" CornerRadius="5,5,5,5"/>
                                <Label Grid.Column="1" Content="Medicine Request" HorizontalAlignment="Left" Margin="59,34,0,0" VerticalAlignment="Top"/>
                                <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Margin="8,35,9,33" CornerRadius="5,5,5,5">
                                    <Image Margin="8,5,132,12" Source="/iconcaustiosn.jpg"/>
                                </Border>
                                <Label Grid.Column="2" Content="Low Stacks Items" HorizontalAlignment="Left" Margin="60,34,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.356,0.403"/>
                                <Border BorderBrush="Black" BorderThickness="1" Grid.Column="3" Margin="10,34,10,34" CornerRadius="5,5,5,5">
                                    <StackPanel>
                                        <Grid Height="50">
                                            <Image HorizontalAlignment="Left" Height="32" Margin="8,6,0,0" VerticalAlignment="Top" Width="41" Source="/persons.jpg"/>
                                            <Label Content="Active Status" HorizontalAlignment="Left" Margin="54,-1,0,0" VerticalAlignment="Top"/>
                                        </Grid>
                                    </StackPanel>
                                </Border>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid>
                                    <Border BorderThickness="1,1,1,1" BorderBrush="#FF595959" CornerRadius="15,15,15,15" Margin="0,0,24,31">
                                        <Grid Margin="0,0,-2,-1">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="3*"/>
                                                <RowDefinition Height="40*"/>
                                            </Grid.RowDefinitions>
                                            <Image HorizontalAlignment="Left" Height="14" Margin="11,5,0,0" VerticalAlignment="Top" Width="73" Source="/sadasdasdasd.jpg"/>
                                            <Image HorizontalAlignment="Left" Height="30" Margin="209,-77,0,0" VerticalAlignment="Top" Width="34" Source="/capsule.jpg"/>
                                        </Grid>
                                    </Border>
                                </Grid>
                                <Grid Grid.Column="1">
                                    <Border BorderBrush="Red" BorderThickness="5,0.5,0.5,0.5" CornerRadius="15,15,15,15" Margin="19,0,0,34">
                                        <Grid Margin="0,0,0,-1">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="3*"/>
                                                <RowDefinition Height="40*"/>
                                            </Grid.RowDefinitions>
                                            <Image HorizontalAlignment="Left" Height="12" Margin="10,0,0,0" VerticalAlignment="Center" Width="77" Source="/sadasdasda.jpg"/>
                                        </Grid>
                                    </Border>
                                </Grid>
                            </Grid>
                        </Grid>
                    </TabItem>

                    <TabItem Header="Medicine" Margin="382,-6,-381,4" BorderBrush="{x:Null}" Background="{x:Null}" Width="85.8466666666667">
                        <Grid>
                            <Grid>
                                <Border BorderBrush="Black" BorderThickness="1" Margin="30,73,34,30" CornerRadius="15,15,15,15">
                                    <Grid Margin="-9,-1,9,1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="81*"/>
                                            <RowDefinition Height="322*"/>
                                        </Grid.RowDefinitions>
                                        <Label Content="Medicine Requests" HorizontalAlignment="Left" Margin="26,10,0,0" VerticalAlignment="Top" Width="203" Foreground="#FF00104D" FontSize="14"/>
                                        <Label Content="Review and approve medicine requests" HorizontalAlignment="Left" Margin="26,31,0,0" VerticalAlignment="Top" Width="244" Foreground="#FFAFAFAF"/>
                                    </Grid>
                                </Border>
                            </Grid>
                        </Grid>
                    </TabItem>

                    <TabItem Margin="0,0,-98,7" Header="Appointment" RenderTransformOrigin="0.5,0.5" Width="86" Height="20" VerticalAlignment="Bottom" mah:HeaderedControlHelper.HeaderForeground="{x:Null}" BorderThickness="5,0,0,0" Background="{x:Null}" BorderBrush="{x:Null}" HorizontalAlignment="Right">
                        <TabItem.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform AngleY="0.794"/>
                                <RotateTransform Angle="-0.719"/>
                                <TranslateTransform Y="1.053"/>
                            </TransformGroup>
                        </TabItem.RenderTransform>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="43*"/>
                                <RowDefinition Height="212*"/>
                            </Grid.RowDefinitions>
                            <StackPanel Margin="10,0,0,0" Grid.RowSpan="2">
                                <Grid Height="70"/>
                                <Border BorderBrush="Black" BorderThickness="1" Height="405" Width="693" CornerRadius="15,15,15,15">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="81*"/>
                                            <RowDefinition Height="322*"/>
                                        </Grid.RowDefinitions>
                                        <Grid>
                                            <Label Content="Pending Appointments&#xD;&#xA;&#xD;&#xA;" HorizontalAlignment="Left" Margin="20,15,0,0" VerticalAlignment="Top" Width="344" Height="32" Foreground="#FF00104D" FontSize="14"/>
                                            <Label Content="Review and manage appointment requests&#xD;&#xA;&#xD;&#xA;" HorizontalAlignment="Left" Margin="20,33,0,0" VerticalAlignment="Top" Width="259" Height="31" Foreground="#FFAFAFAF"/>
                                        </Grid>
                                    </Grid>
                                </Border>
                            </StackPanel>
                        </Grid>
                    </TabItem>

                    <TabItem Margin="351,-6,-352,4" Header="Students" Width="85.8466666666667" Background="{x:Null}" BorderBrush="{x:Null}">
                        <Grid>
                            <Border BorderBrush="Black" BorderThickness="1" Margin="28,72,31,35" CornerRadius="15,15,15,15">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="31*"/>
                                        <RowDefinition Height="177*"/>
                                    </Grid.RowDefinitions>
                                    <Label Content="Student Management" HorizontalAlignment="Left" Margin="18,10,0,0" VerticalAlignment="Top" Width="224" Foreground="#FF00104D" FontSize="14"/>
                                    <Label Content="Manage student accounts and records" HorizontalAlignment="Left" Margin="18,26,0,0" VerticalAlignment="Top" Width="217" Foreground="#FFAFAFAF"/>
                                    <Image HorizontalAlignment="Center" Height="62" Margin="0,56,0,0" Grid.Row="1" VerticalAlignment="Top" Width="85" Source="/123123123.jpg"/>
                                    <Label Content="Comprehensive student account management system" HorizontalAlignment="Center" Margin="0,117,0,0" Grid.Row="1" VerticalAlignment="Top" Width="297"/>
                                    <Button Content=""
        HorizontalAlignment="Center"
        Margin="0,148,0,0"
        Grid.Row="1"
        VerticalAlignment="Top"
        Width="114"
        Height="31" Cursor="Hand">
                                        <Button.Template>
                                            <ControlTemplate TargetType="Button">
                                                <Border CornerRadius="4"
                    BorderThickness="1"
                    BorderBrush="Black"
                    Background="{TemplateBinding Background}" Margin="0,0,0,12">
                                                    <ContentPresenter HorizontalAlignment="Center"
                                  VerticalAlignment="Center"/>
                                                </Border>
                                            </ControlTemplate>
                                        </Button.Template>

                                        <Button.Background>
                                            <ImageBrush ImageSource="/dsadasdasdasda.jpg" Stretch="Fill"/>
                                        </Button.Background>
                                    </Button>
                                    <Label Content="• Add new student accounts with comprehensive information&#xD;&#xA;    • Edit existing student details and medical information&#xD;&#xA;         • View enrollment analytics and program statistics&#xD;&#xA;             • Search and filter students by various criteria" HorizontalAlignment="Left" Margin="184,170,0,0" Grid.Row="1" VerticalAlignment="Top" Width="357" Height="79" Foreground="#FFAFAFAF"/>
                                </Grid>
                            </Border>
                        </Grid>
                    </TabItem>

                </TabControl>
            </Grid>
        </Grid>
    </Border>
</Window>